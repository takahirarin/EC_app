<p>ジャンル一覧・追加</p>

<%= form_with model: @genre, url: admin_genres_path do |f| %>
<div>ジャンル名</div>
<div>
<%= f.label :name %>
<%= f.text_field :name %>
</div>
<%= f.submit %>
<% end %>

<table>
<div>ジャンル</div>
<% @genres.each do |genre| %>
<tr>
<%= genre.name %>
<%= link_to "編集", edit_admin_genre_path(genre) %>
</tr>
</table>
<% end %>