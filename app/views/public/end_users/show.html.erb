<table>
	<tbody>
		<tr>
			<td>氏名</td>
			<td><%= current_end_user.second_name + current_end_user.first_name %></td>
		</tr>
		<tr>
		    <td>カナ</td>
			<td><%= current_end_user.second_name_kana + current_end_user.first_name_kana %></td>
